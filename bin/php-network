#!/bin/bash

cmd=$(docker network ls | grep sign-networks)

## execution de PHP de mani√®re autonome (pas besoin d'un environnement de containers)
docker run \
    --init \
    --rm \
    -ti \
    --name nextsign-php-interpreter-running-script \
    -v "$PWD":"$PWD" \
    --user 1000:1000 \
    -w "$PWD" \
    --network docker_nextsign-networks \
    nextsign-php-interpreter php $@
